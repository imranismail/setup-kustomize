    "@octokit/plugin-throttling": "^3.4.1",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "~17.0",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
