{
  "name": "setup-kustomize",
  "version": "1.6.1",
  "private": true,
  "description": "Github action to setup-kustomize",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
